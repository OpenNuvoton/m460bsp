{
    "debug.hideSlowPreLaunchWarning": true,
    "cmake.copyCompileCommands": "${workspaceFolder}/build/armclang/compile_commands.json",
    "clangd.arguments": [
        "--compile-commands-dir=${workspaceFolder}/build/armclang", "--background-index", "--clang-tidy"
    ],
    "files.associations": {
        "dfu_transfer.h": "c",
        "stdio.h": "c",
        "numicro.h": "c",
        "string.h": "c"
    },
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "cmsis-csolution.actions": [
        {
            "path": "SampleCode/ISP/ISP_DFU/VSCode/ISP_DFU.csolution.yml",
            "target": "ARMCLANG",
            "config": {
                "debug": "Embedded Debug",
                "run": "arm-debugger.flash: Flash Device"
            }
        },
        {
            "path": "SampleCode/ISP/ISP_DFU_20/VSCode/ISP_DFU_20.csolution.yml",
            "target": "ARMCLANG",
            "config": {
                "debug": "Embedded Debug",
                "run": "arm-debugger.flash: Flash Device"
            }
        },

        {
            "path": "SampleCode/StdDriver/FMC_IAP/VSCode/FMC_IAP.csolution.yml",
            "target": "ARMCLANG",
            "config": {
                "debug": "Embedded Debug",
                "run": "arm-debugger.flash: Flash Device"
            }
        },
        {
            "path": "SampleCode/StdDriver/FMC_MultiBoot/VSCode/FMC_MultiBoot.csolution.yml",
            "target": "GNUC",
            "config": {
                "debug": "Embedded Debug",
                "run": "arm-debugger.flash: Flash Device"
            }
        },
        {
            "path": "SampleCode/StdDriver/FMC_MultiWordProgram/VSCode/FMC_MultiWordProgram.csolution.yml",
            "target": "ARMCLANG",
            "config": {
                "run": "arm-debugger.flash: Flash Device"
            }
        }
    ]
}